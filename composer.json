{
    "name": "rokde/wordpress-to-markdown",
    "description": "A cli application to convert wordpress backup xml into markdown files for posts and pages",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "wordpress", "markdown"],
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rokde/wordpress-to-markdown/issues",
        "source": "https://github.com/rokde/wordpress-to-markdown"
    },
    "authors": [
        {
            "name": "Robert Kummer",
            "email": "rok@ipunkt.biz"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-simplexml": "*",
        "laravel-zero/framework": "5.6.*",
        "league/html-to-markdown": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["wordpress-to-markdown"]
}
